{
  "name": "test-backend",
  "version": "0.0.1",
  "description": "",
  "author": "Long",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start -b swc",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/src/main.js",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "typecheck": "tsc --noEmit",
    "test": "jest --config ./jest.config.js",
    "test:cov": "npm run test --coverage",
    "test:e2e": "jest --config ./jest.e2e.js",
    "test:watch": "jest --watch",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "seed": "ts-node -r tsconfig-paths/register src/database/seeders/seed.command.ts",
    "migration:run": "typeorm-ts-node-commonjs migration:run -d ./src/database/data-source.ts",
    "migration:generate": "typeorm-ts-node-commonjs -d ./src/database/data-source.ts migration:generate ./src/migrations/$npm_config_name",
    "migration:create": "typeorm-ts-node-commonjs migration:create ./src/migrations/$npm_config_name",
    "migration:revert": "typeorm-ts-node-commonjs -d ./src/database/data-source.ts migration:revert",
    "seed:full": "npm run seed full",
    "seed:all": "npm run seed all",
    "seed:users": "npm run seed users",
    "seed:metrics": "npm run seed metrics",
    "seed:clear": "npm run seed clear"
  },
  "dependencies": {
    "@apollo/server": "^4.9.4",
    "@nestjs/apollo": "^12.0.9",
    "@nestjs/axios": "^3.0.0",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.0.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/graphql": "^12.0.9",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/swagger": "^7.1.13",
    "@nestjs/terminus": "^10.1.1",
    "@nestjs/typeorm": "^10.0.0",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/api-logs": "^0.203.0",
    "@opentelemetry/semantic-conventions": "^1.36.0",
    "axios": "^1.5.1",
    "bcrypt": "^5.1.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "compression": "^1.7.4",
    "graphql": "^16.8.1",
    "graphql-type-json": "^0.3.2",
    "helmet": "^7.0.0",
    "moment": "^2.29.4",
    "mysql2": "^3.4.2",
    "nest-commander": "^3.18.0",
    "pg": "^8.11.3",
    "pm2": "^5.3.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "typeorm": "^0.3.17",
    "webpack": "^5.88.2"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.4.20",
    "@swc/cli": "^0.1.65",
    "@swc/core": "^1.13.3",
    "@types/bcrypt": "^5.0.0",
    "@types/compression": "^1.7.3",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.3.1",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.59.11",
    "@typescript-eslint/parser": "^5.59.11",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.5.0",
    "prettier": "^2.8.8",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
